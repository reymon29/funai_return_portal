<div class="container">
<%= simple_form_for [:shipping, @return] do |f| %>
  <%= f.error_notification %>

  <h4 class="fs-title">Funai model</h4>
  <%= f.input :rma_status, required: true, collection: ["RMA Approved, assigning shipping", "RMA Denied, past return period", "RMA Denied, not enough information"]%>
  <%= f.input :rma_number, required: true %>
  <%= f.button :submit, "Submit", class: "btn btn-primary" %>
  <%= link_to 'Cancel', dashboard_path, class: "btn btn-warning" %>
  <% end %>
</div>


